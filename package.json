{
  "name": "@btnguyen2k/checksum",
  "version": "0.1.0",
  "description": "Calculate checksum for any Javascript value",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./src/index.js",
  "browser": "./lib/index.umd.js",
  "files": [
    "lib/",
    "src/",
    "README.md",
    "LICENSE.md",
    "RELEASE-NOTES.md"
  ],
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "npm run lint && npm run test:cover && babel ./src -d ./lib",
    "lint": "eslint .",
    "test": "jest --verbose",
    "test:cover": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btnguyen2k/js-checksum.git"
  },
  "keywords": [
    "checksum"
  ],
  "author": "Thanh Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/btnguyen2k/js-checksum/issues"
  },
  "homepage": "https://github.com/btnguyen2k/js-checksum#readme",
  "dependencies": {
    "crypto-js": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "babel-jest": "^29.6.1",
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest-cli": "^29.6.1",
    "semantic-release": "^21.0.7"
  }
}
